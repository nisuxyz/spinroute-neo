# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Server
PORT=3000
NODE_ENV=development

# Apple App Store
APPLE_BUNDLE_ID=com.yourcompany.yourapp
APPLE_SHARED_SECRET=your_shared_secret_from_app_store_connect
APPLE_ENVIRONMENT=sandbox
# Optional: For App Store Server API (advanced features)
APPLE_KEY_ID=your_key_id
APPLE_ISSUER_ID=your_issuer_id
APPLE_PRIVATE_KEY=your_private_key_content
APPLE_APP_ID=your_app_apple_id

# Google Play
GOOGLE_PACKAGE_NAME=com.yourcompany.yourapp
GOOGLE_CLIENT_EMAIL=your_service_account_email
GOOGLE_PRIVATE_KEY=your_service_account_private_key